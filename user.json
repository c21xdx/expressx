{
	"log": {
		"loglevel": "warning"
	},
	"routing": {
		"rules": [{
			"type": "field",
			"protocol": [
				"bittorrent"
			],
			"outboundTag": "block"
		}]
	},
	"inbounds": [{
		"listen": "0.0.0.0",
		"port": 8082,
		"protocol": "vless",
		"settings": {
			"clients": [{
				"id": "app_passid"
			}],
			"decryption": "none"
		},
		"streamSettings": {
			"network": "xhttp",
			"xhttpSettings": {
				"path": "/api"
			}
		},
		"sniffing": {
			"enabled": true,
			"destOverride": [
				"http",
				"tls",
				"quic"
			]
		}
	}],
	"outbounds": [{
			"protocol": "freedom"
		},
		{
			"protocol": "blackhole",
			"tag": "block"
		}
	]
}
